{"ast":null,"code":"import moment from 'moment';\nimport { TagSelect, StandardFormRow, Ellipsis, AvatarList } from '@/components';\nvar TagSelectOption = TagSelect.Option;\nvar AvatarListItem = AvatarList.Item;\nexport default {\n  components: {\n    AvatarList: AvatarList,\n    AvatarListItem: AvatarListItem,\n    Ellipsis: Ellipsis,\n    TagSelect: TagSelect,\n    TagSelectOption: TagSelectOption,\n    StandardFormRow: StandardFormRow\n  },\n  data: function data() {\n    return {\n      data: [],\n      form: this.$form.createForm(this),\n      loading: true\n    };\n  },\n  filters: {\n    fromNow: function fromNow(date) {\n      return moment(date).fromNow();\n    }\n  },\n  mounted: function mounted() {\n    this.getList();\n  },\n  methods: {\n    handleChange: function handleChange(value) {\n      console.log(\"selected \".concat(value));\n    },\n    getList: function getList() {\n      var _this = this;\n      this.$http.get('/list/article', {\n        params: {\n          count: 8\n        }\n      }).then(function (res) {\n        console.log('res', res);\n        _this.data = res.result;\n        _this.loading = false;\n      });\n    }\n  }\n};","map":{"version":3,"mappings":"AAoFA;AACA;AACA;AACA;AAEA;EACAA;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACAC;IACA;IACAC;MAAA;MACA;QAAAC;UAAAC;QAAA;MAAA;QACAH;QACA;QACA;MACA;IACA;EACA;AACA","names":["components","AvatarList","AvatarListItem","Ellipsis","TagSelect","TagSelectOption","StandardFormRow","data","form","loading","filters","fromNow","mounted","methods","handleChange","console","getList","params","count"],"sourceRoot":"src/views/list/search","sources":["Projects.vue"],"sourcesContent":["<template>\n  <div>\n    <a-card :bordered=\"false\" class=\"ant-pro-components-tag-select\">\n      <a-form :form=\"form\" layout=\"inline\">\n        <standard-form-row title=\"所属类目\" block style=\"padding-bottom: 11px;\">\n 